  return {
    checkLabels : function(labels) {
      checkLabelsForIvw(labels);
      for (var i=labels.length-1;i >= 0; i--) {
        if (labels[i] == 'noadsense') {
          noAdsenseLabelled = true;
        }
        if (labels[i] == 'PamS') {
          PamSLabelled = true;
        }
      }
      labelsAreChecked = true;
