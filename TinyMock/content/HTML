    <script type="text/javascript">
      // Zieht Faktillon-Bild aus tumblr-Feed (json) und verlinkt auf den tumblr-Post
      function getNewestFaktillonImg(pictype, container, linktype) {
        var post = tumblr_api_read.posts[1];
        var ImageURL = post[pictype];
        var ImageLink = post[linktype]
        console.log(ImageLink)
        $(container).append($("<a>").attr("href", ImageLink).append($("<img>").attr("src", ImageURL)));
      }

      getNewestFaktillonImg("photo-url-400", "#recentPosts", "url");
    </script>

    <style>
      #recentPosts a,
      #recentPosts a img {
        width: 100%;
      }
    </style>
